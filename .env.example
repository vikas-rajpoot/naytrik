# Gemini API Key (required for recording)
GEMINI_API_KEY=your_gemini_api_key_here

# Model selection (optional)
# Options: gemini-2.5-computer-use-preview-10-2025, gemini-2.0-flash-exp, gemini-1.5-pro
GEMINI_MODEL_NAME=gemini-2.5-computer-use-preview-10-2025

# Optional: Use VertexAI instead of Gemini API
USE_VERTEXAI=false
VERTEXAI_PROJECT=your_project_id
VERTEXAI_LOCATION=us-central1

# Optional: Browserbase integration
BROWSERBASE_API_KEY=
BROWSERBASE_PROJECT_ID=

# Recording options
RECORD_SCREENSHOTS=false
SCREENSHOT_QUALITY=80

# Playback options
DEFAULT_TIMEOUT=30000
HEADLESS=false
SLOW_MO=0

# Storage
WORKFLOWS_DIR=./workflows
